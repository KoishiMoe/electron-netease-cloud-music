{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "Bundler",
        "experimentalDecorators": true,
        "paths": {
            "@/*": [
                "./src/renderer/*"
            ],
            "assets/*": [
                "./assets/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}